{
    "name": "peteck/podio-php-ezauth",
    "description": "Makes is possible to make one-line auth with the Podio-PHP library",
    "keywords": ["podio", "ezauth", "podio-php"],
    "license": "MIT",
    "version": "1.0.1",
    "require":
    {
        "php": ">=5.3",
        "podio/podio-php": "dev-master"
    },
    "replace":
    {
        "peteck/podio-php-ezauth": "*"
    },
    "autoload":
    {
        "classmap": ["lib/"]
    },
    "type": "library"
}